sudo dnf install podman
sudo dnf install tmux
tmux ^b d
tmux attach -t 0
podman build -t instructlab .
podman run -it --rm instructlab
source venv/bin/activate
ilab model serve &>~/log-model-serve &
ilab data generate --pipeline simple --batch-size=0 --enable-serving-output &>~/log-data-generate  &
